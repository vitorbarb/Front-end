Algoritmo "programa2"

Var
  base: inteiro
  altura: inteiro
  area: inteiro

Inicio
 escreval("Escreva o tamanho da base do retangulo")
 leia(base)
 limpatela()


 escreval("Escreva o tamanho da altura do retangulo")
 leia(altura)
 limpatela()
 area <- base*altura
 
 escreva("A area total do retangulo Ã© de: ", area)

Fimalgoritmo
